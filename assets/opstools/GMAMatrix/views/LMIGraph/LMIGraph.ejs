
<div class="gmamatrix-container gmamatrix-dashboard-container">
    <h5><%= title %></h5>
    <div class="row staff">
        <div class="col-xs-9">
            <span class="gma-dashboard-label">Staff</span>
            <div class="gmaSparkline" style="height:24px;width:50px;"></div>
        </div>
        <div class="col-xs-3">
            <span class="pull-right field-total">986</span>
        </div>
    </div>
    <div class="row disciple">
        <div class="col-xs-9">
            <span class="gma-dashboard-label">Disciple</span>
            <div class="gmaSparkline" style="height:24px;width:50px;"></div>
            
        </div>
        <div class="col-xs-3">
            <span class="pull-right field-total">1255</span>
        </div>
    </div>
    <div class="row total">
        <div class="col-xs-9 opsportal-border-top">
            <span class="hris-tooltip tt gma-highlight field-ratio" data-placement="top" data-original-title="Staff to Disciple Ratio">1:1.2</span>
        </div>
        <div class="col-xs-3 opsportal-border-top">
            <span class="pull-right gma-highlight field-total">2241</span>
        </div>
    </div>
</div>

